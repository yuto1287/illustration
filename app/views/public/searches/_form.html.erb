<% if user_signed_in? %>
 <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.search_field :content, value: params[:content] %>
    <%= f.select :model, options_for_select({ "User" => "user", "List" => "list" }, selected: params[:model]) %>
    <%= f.select :method, options_for_select({ "完全一致" => "perfect", "前方一致" => "forward", "後方一致" => "backword", "部分一致" => "partial"}, selected: params[:method]) %>
    <%= f.submit '検索' %>
  <% end %>
<% end %>
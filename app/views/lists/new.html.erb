<h1>投稿フォーム</h1>
<%= form_with model: @list do |f| %>
  <%= render 'layouts/error_messages', resource: f.object %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>イラスト</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>説明</h4>
  <%= f.text_area :caption %>
  <%= f.submit '投稿' %>

<% end %>
